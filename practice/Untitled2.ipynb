   "source": [
    "import numpy as np\n",
    "in1=input(\"행렬 1: \")\n",
    "alist=in1.split()\n",
    "a=np.array(alist,dtype='i').reshape(3,3)\n",
    "in2=input(\"행렬 2: \")\n",
    "blist=in2.split()\n",
    "b=np.array(blist,dtype='i').reshape(3,3)\n",
    "print(\"행렬의 합: \\n\",a+b)\n",
    "print(\"행렬의 차: \\n\",a-b)\n",
    "print(\"행렬의 곱: \\n\",a*b)"
   ]
